name:               miso-invaders
version:            0.1
build-type:         Simple
cabal-version:      >=1.10

executable miso-invaders
    main-is:            Main.hs
    hs-source-dirs:     src
    ghc-options:        -Wall -O
    ghcjs-options:      -dedupe
    build-depends:      base, containers, ghcjs-base, miso
    default-language:   Haskell2010

